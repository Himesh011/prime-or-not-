#include<iostream>

using namespace std;

int main()

{

    int no,i, count=0;

    cout << "\nenter the number\n";

    cin >> no;

    for(int i=1;i<=no;i++)

    {

      if(no%i==0) 

       {

            count++;

       }

    }

    if(count==2)

    {

        cout<<endl<<no<<"\tis prime number";

    

    }

    else

    {

        cout<<endl<<no<<"\tis not prime number";

    }



}
